/* calculate the area of circle and modify the same program to calculate the volume of cylinder
given its radius and height*/

#include<stdio.h>
int main()
{
	int r = 4;
	printf("The area of circle is %f\n",3.14*r*r);
	
	int h = 5;
	printf("The volume of cylinder if %f\n",3.14*r*r*h);
	
	return(0);
}
